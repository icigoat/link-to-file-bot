{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "TG_API_ID": "@tg_api_id",
    "TG_API_HASH": "@tg_api_hash", 
    "TG_SESSION_STRING": "@tg_session_string"
  }
}